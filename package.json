{
    "name": "api-project-test",
    "scripts": {
        "format": "prettier --write .",
        "test": "jest",
        "lint": "eslint src/ --cache --fix",
        "build": "tsc",
        "build:dev": "tsc --watch",
        "start": "node --enable-source-maps dist/server.js",
        "start:dev": "nodemon --watch dist/ --watch .env",
        "dev": "npm run build && concurrently 'npm:build:dev' 'npm:start:dev'",
        "web": "serve -p 8080 web/"
    },
    "engines": {
        "node": ">=16"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Simulaemanuele/api-project-test.git"
    },
    "bugs": {
        "url": "https://github.com/Simulaemanuele/api-project-test/issues"
    },
    "homepage": "https://github.com/Simulaemanuele/api-project-test#readme",
    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jest": "^29.0.3",
        "@types/node": "^18.7.14",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "concurrently": "^7.4.0",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^28.1.3",
        "jest-mock-extended": "^2.0.7",
        "nodemon": "^2.0.19",
        "prettier": "2.7.1",
        "prisma": "^4.3.1",
        "rxjs": "^7.5.6",
        "serve": "^14.0.1",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.8",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "@prisma/client": "^4.3.1",
        "@sinclair/typebox": "^0.24.41",
        "ajv-formats": "^2.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1",
        "express-json-validator-middleware": "^3.0.1"
    }
}
